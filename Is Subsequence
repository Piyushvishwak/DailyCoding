class Solution {
public:
    bool isSubsequence(string s, string t) {
        int i=0;
        int count=0;
        for(char c :t){
            if(c==s[i]){
                i++;
                count++;
            }
        }
        if(count==s.length()){
            return true;
        }
        return false;
        
    }
};
